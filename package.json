{
  "name": "customerio-reactnative",
  "version": "4.2.4",
  "description": "Official Customer.io SDK using React Native. Track customers and send messages to your iOS/Android apps.",
  "source": "./src/index.ts",
  "main": "./lib/module/index.js",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./lib/module/index.js",
  "cioNativeiOSSdkVersion": "= 3.8.1",
  "iosSwiftVersion": "5.0",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "pre-deploy": "npm run typescript",
    "test": "jest",
    "typecheck": "tsc",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean_ios_apn": "del-cli example/ios_native/APN/Pods example/ios_native/APN/build example/ios",
    "clean_ios_fcm": "del-cli example/ios_native/FCM/Pods example/ios_native/FCM/build example/ios example/ios_native/FCM/Podfile.lock",
    "clean_node_modules": "del-cli node_modules example/node_modules package-lock.json example/package-lock.json",
    "clean_android": "del-cli android/build example/android/build example/android/app/build",
    "clean": "npm run clean_ios_fcm && npm run clean_ios_apn && npm run clean_android && del-cli lib && npm run clean_node_modules",
    "clean_and_build_all": "npm run clean && npm install && cd example && npm install",
    "lint:report": "npm run lint -- --output-file eslint_report.json --format json",
    "prepare": "bob build",
    "postinstall": "node src/postInstall.js",
    "format": "npx prettier --write \"src/**/*.tsx\" && (npm run lint || true)"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/customerio/customerio-reactnative",
  "author": "Customer.io Mobile Development Team (https://customer.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/customerio/customerio-reactnative/issues"
  },
  "homepage": "https://github.com/customerio/customerio-reactnative#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.0",
    "@evilmartians/lefthook": "^1.11.10",
    "@react-native-community/cli": "18.0.0",
    "@react-native/eslint-config": "0.79.1",
    "@react-native/metro-config": "0.79.1",
    "@react-native/typescript-config": "0.79.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "commitlint": "^19.8.0",
    "del-cli": "^5.1.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.6.3",
    "prettier": "^3.5.3",
    "react": "19.1.0",
    "react-native": ">=0.79.1",
    "react-native-builder-bob": "^0.40.6",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "Apps/",
    "example/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module"
      ],
      [
        "commonjs"
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}